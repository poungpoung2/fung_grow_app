{
  "expo": {
    "name": "Fung Grow",
    "slug": "fung-grow",
    "scheme": "untitled-app",
    "description": "My app powered by Draftbit",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/app/icon.png",
    "backgroundColor": "#FBFCFD",
    "splash": {
      "image": "./assets/app/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5A45FF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "ALL"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0"
          }
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.draftbit.untitledapp",
      "buildNumber": "1.0.0",
      "infoPlist": {},
      "usesIcloudStorage": false,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 1,
      "package": "com.draftbit.untitledapp",
      "permissions": [],
      "adaptiveIcon": {}
    },
    "androidStatusBar": {},
    "androidNavigationBar": {},
    "web": {
      "themeColor": "#000000"
    },
    "notification": {
      "androidMode": "default"
    },
    "extra": {
      "eas": {
        "projectId": ""
      }
    }
  }
}